.page-head {
    position: fixed;
    display: flex;
    width: 100%;
    height: 44px;
    line-height: 44px;
    z-index: 3;
    background-color: #f2f2f2;
}

.page-head-normall {
    position: relative;
    display: flex;
    padding: 0 10px;
    justify-content: space-between;
    align-items: center;
    width: 100%;
    background-color: #f2f2f2;
}

.page-head-normall a {
    color: #000;
}

.hide-header {
    background-color: rgba(0, 0, 0, 0);
}
