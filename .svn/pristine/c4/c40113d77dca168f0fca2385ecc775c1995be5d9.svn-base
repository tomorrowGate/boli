html,body{
    height: 100%;
}
.page-head{
    display: flex;
    height: 44px;
    line-height: 44px;
}
.head-center{
    font-weight: bold;
    margin: 0 auto;
}
.back{
    position: absolute;
    left: 10px;
    top:50%;
    transform: translateY(-50%)
}
html,body,.wrap{
    width: 100%;
    height: 100%;
    background: #fff;
    font-size: 14px;
}
/* tabbar */
.ui-tab-nav li{
    font-weight: bold;

}
.ui-tab-nav li.current {
    color: #6eb92c;
}
.ui-tab-nav li.current>p:before, .ui-tab-nav li.current>span:before {
    content: "";
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    width: 50%;
    height: 2px;
    transform: translateX(50%);
    background-color: #6eb92c;
}
.list-flex{
    display: flex;
    justify-content: space-between;
    line-height: 28px;
}