/*头部*/

.page-head {
    display: flex;
    height: 44px;
    line-height: 44px;
}

.head-center {
    margin: 0 auto;
}

.back {
    position: absolute;
    left: 10px;
    top: 50%;
    transform: translateY(-50%)
}
.whole{
  display: flex;
  align-items: flex-start;

}
/*导航条 */
.navigation{
  width: 25%;
  height: 100%;
  border-right: 8px solid #efefef;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  padding-top: 10px;

}
.navItem{
  width: 100%;
  height: 40px;
  text-align: center;
  line-height: 40px;
}
.navItemFirst{
  background: #efefef;
}
.active{
  color: #77bc3b;
  position: relative;
}
.active::before{
display: inline-block;
content: "";
width: 0;
height: 16px;
border:1px solid #77bc3b;
position: absolute;
top: 11px;
left: 22px;
}
/*菜单选项栏*/
.optionBar{
  width: 70%;
  display: flex;
   flex-wrap:wrap;
     padding-top: 10px;
}
.optionBarItem{
width: 33%;
height: 40px;
text-align: center;
line-height: 40px;
color: #b4b4b4;
}
