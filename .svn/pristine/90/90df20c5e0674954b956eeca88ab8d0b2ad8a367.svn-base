/*遮罩层*/
.mask {
	position: absolute;
	top: 0;
	left: 0;
	z-index: 12;
	width: 100%;
	height: 100%;
	background-color: rgba(0, 0, 0, .5);
}

@media (max-width: 720px) {
	.mask {
		position: fixed;
	}
}

.border-b {
	border-bottom: 1px solid #f2f2f2;
}

.flex {
	display: flex;
	display: -webkit-flex;
	-webkit-flex-flow: row wrap;
	/*flex属性，规定灵活项目的方向和是否拆行或拆列*/
}

.cross-middle {
	/*子元素垂直居中*/
	display: flex;
	display: -webkit-flex;
	display: -moz-flex;
	align-items: center;
	-webkit-align-items: center;
	-moz-align-items: center;
}

.tapmodeclass {
	background: rgba(0, 0, 0, 0.3);
}