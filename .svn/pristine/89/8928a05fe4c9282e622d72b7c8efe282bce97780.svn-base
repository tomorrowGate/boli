/* 头部导航 */
.page-head{
    display: flex;
    height: 44px;
    line-height: 44px;
    display: none;
}
.head-center{
    width: 100%;
   display: flex;
   justify-content: space-around;
   margin-left: 45px;
   
}
.back{
    position: absolute;
    left: 10px;
    top:50%;
    transform: translateY(-50%)
}
/* 视频详情 */

 .detail-teach-videobox{
     height: 250px;
     position: relative;
     background: skyblue;
     margin-bottom: 10px;
 }
 .video-tag{
    line-height: 16px;
    padding: 0 10px;
 }
 .icon-back{
     position: absolute;
     top: 6px;
     left: 10px;
 }
 .detail-video-tag {
     position: absolute;
     top:6px;
     right:10px;
     
 }
 .navBtn{
     position: absolute;
     bottom: 6%;
     left: 36%;
     font-size: 0.6em;
 }
.videoBtn{
 padding: 4px 14px;
 background: #6fba2c;
 border-radius: 12px;
 color: #fff;
 }
 .imgBtn{
   padding: 4px 14px;
   background-color: rgba(255,255,255,.3);
   border-radius: 12px;
   margin-left: 5px;
 } 
/* 商品价格 */
.price{
    height: 105px;
    padding: 2px 30px 2px 10px;
    border-bottom: 8px solid #f2f2f2; 
}
.priceDiscount{
    color: #fe1111;
}
.priceNum{
    margin-left: -5px;
    font-weight: bold;
}
.priceDiscount .priceDel{
    font-size: 0.6em;
    color:#b8b8b8;
    text-decoration: line-through;
    margin: 0 6px 0 0;
}
.priceMarin{
    margin: 1px 0 5px 0;
}
.priceLogo{
 padding: 2px 14px;
 background: #6fba2c;
 border-radius: 12px;
 color: #fff;
 font-size: 0.6em;
 line-height: 1em;
 margin-right: 5px;
}
 .priceSmall{
     color: #fe1111;
     font-size: 0.9em;
 }
 /* 发货优惠 */
 .consignment{
     padding: 0 10px;
     height: 45px;
     font-size: 0.9em;
     line-height: 45px;
     border-bottom: 1px solid #f2f2f2;
     position: relative;
 }
 .tag{
     color: #b8b8b8;
     margin-right: 10px;
 }
 .add{
     margin: 0 5px;
 }
 .salesVolume{
     color: #b8b8b8;
     position: absolute;
     right: 10px;
 }
 .discount{
    padding: 0 18px;
    height: 45px;
    font-size: 0.9em;
    line-height: 45px;
    border-bottom: 6px solid #f2f2f2;
    position: relative;
 }
 .coupon{
    color: #fe1111;
 }
 .icon-right{
 position: absolute;
 right: 10px;
 }
 /* 保障 */
 .guarantee{
     font-size: 0.9em;
     padding: 15px 10px;
     line-height: 18px;
     position: relative;
     border-bottom: 6px solid #f2f2f2;
 }
 .guaranteeText{
     color: #6fba2c;
     margin-left: 45px;
 }
 .icon-Location{
     position: absolute;
     right: 10px;
     top: 14px;
 }
 /* 宝贝评价 */
 .evaluate{
     padding: 0 10px;
     font-size:0.8em;
 }
 .evaluateHead{
     line-height: 45px;
     position: relative;
     font-size: 15px;
 }
 .whole{
   position: absolute;
   right: -7px;
   color: #6fba2c;
 }
 .icon-left{
     margin-left: 6px;
 }
.message{
    line-height: 24px;
    margin-bottom: 10px
}
 .flex{
     display: flex;
     align-items: center;
     
 }
 .circle{
     height: 30px;
     width: 30px;
     background: #f2f2f2;
     border-radius: 50%;
     margin-right: 10px;
 }
 .circleName{
     color: #b8b8b8;
 }
 .circleText{
     margin-left: 38px;
 }
 /* 商品详情 */
 .commodityHead{
     background: #f2f2f2;
     line-height: 44px;
 }
 .commodityTitle{
     margin: 0 20px;
     font-weight: bold;
 }
 .commoditySpacing{
    text-align: center;
 }
 /* 底部导航条 */
 .footNav{
     width: 100%;
     display: flex;
     justify-content: space-around;
     font-size: 0.9em;
     height: 50px;
     position: fixed;
     bottom:0;
     background: #fff;
 }
 .footNav-item{
     width: 25%;
     height: 100%;
     text-align: center;
     
 }
 .footNav-purchase{
     font-size: 18px;
     color: #fff;
     background: #2b2b2b;
     line-height: 50px;
 }
.footNav-share{
    font-size: 18px;
    color: #fff;
    background: #6fba2c;
    line-height: 50px;
}
.footNav-top{
    margin: 5px 0;
    text-align: center;
    color: #b8b8b8;
    width: 25%;
}
.footNav-bor{
    border-right: 1px solid #b8b8b8;
}