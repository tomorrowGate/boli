html,body{
    background: #f2f2f2;
}
.page-head{
    display: flex;
    height: 44px;
    line-height: 44px;
}
.head-center{
    margin: 0 auto;
}
.back{
    position: absolute;
    left: 10px;
    top:50%;
    transform: translateY(-50%)
}
/* 新增地址 */
.formBox{
    background: #fff;
    padding: 10px;
}
.formName{
    border-bottom: 1px solid #f2f2f2;
    line-height: 45px;
}
input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {
    color: #999;
    font-size: 0.8em;
  }
  .formRegion{
      font-size: 0.9em;
      display: flex;
      justify-content: space-between;
      border-bottom: 1px solid #f2f2f2;
      line-height: 45px;
      
  }
  .iconRight{
      margin-left: 15px;
  }