            <div class="swiper-wrapper" id="forumRecommend">
                <div class="swiper-slide" tapmode>
                    <div class="forum-foot-imgbox">
                        <img src="" alt="">
                    </div>
                </div>
                <div class="swiper-slide" tapmode>14</div>
                <div class="swiper-slide" tapmode>153</div>
                <div class="swiper-slide" tapmode>14</div>
                <div class="swiper-slide" tapmode>153</div>
