html,
body {
    background: #f2f2f2;
}

/* 头部 */
.page-head {
    display: flex;
    height: 44px;
    line-height: 44px;
}

.head-center {
    margin: 0 auto;
    font-weight: bold;
}

.back {
    position: absolute;
    left: 10px;
    top: 55%;
    transform: translateY(-50%)
}

.headBg {
    background: #6fba2c;
    text-align: center;
    line-height: 85px;
}

.headBtn {
    color: #fff;
    border: 1px solid #fff;
    border-radius: 5px;
    padding: 5px 15px;
}

/* 快递运输状态 */
.express {
    width: 94%;
    box-sizing: border-box;
    background: #fff;
    border-radius: 5px;
    margin: 8px auto;
    font-size: 0.8em;
}

.expressAdd {
    color: #6fba2c;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 10px 10px 4px 10px;
}

.addRight {
    padding-right: 8px;
}

.addLeft {
    padding-left: 5px;
}

.addTime {
    margin-left: 10%;
    line-height: 22px;
    color: #ccc;
}

/* 收货地址 */
.receiving {
    width: 94%;
    box-sizing: border-box;
    background: #fff;
    margin: 0 auto;
    font-size: 0.8em;
    border-radius: 5px;
}

.receivingWX {
    color: #ccc;
    margin-left: 10%;
    line-height: 25px;
}

.receivingAdd {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    line-height: 20px;
    padding: 0 0 10px 10px;
}

.receivingLocation {
    margin-left: 8px;
}

.receivingName {
    font-size: 1.2em;
    padding-right: 10px;
}

.phone {
    color: #ccc;
}

/* 已选商品 */
.after-goods{
    box-sizing: border-box;
    width: 94%;
    background: #fff;
    margin: 8px auto;
    border-radius: 5px;
    padding: 15px 8px 5px 8px;
}
.after-goods-item {
    display: flex;
    padding-bottom: 6px;
    align-items: center;
}

.shopIcn {
    align-items: center;
}

.chef-form-leftimg {
    width: 120px;
    height: 110px;
    margin-right: 10px;
    border-radius: 4px;
    background: skyblue;
}

.chef-form-leftimg img {
    width: 100%;
    height: 100%;
    border-radius: 4px;
}

.chef-right-content {
    line-height: 20px;
    width: 70%;
    position: relative;
}


.chef-head1 {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.how {
    color: #ccc;
}

.chef-head2 {
    color: #a3a3a3;
    font-size: 0.6em;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    line-height: 24px;
}

.chef-head3 {
    font-size: 0.6em;
    color: #a3a3a3;
}

.forum-foot-bg {
    background: #d96b6c;
    padding: 0 5px;
    margin-right: 2px;
    color: #fff;
    border-radius: 4px;
}

.chef-head4 {
    margin-top: 30px;
    display: flex;
    justify-content: space-between;
}

.swiper-red {
    color: #ff0000;
    position: relative;

}

.swiper-del {
    margin-left: 3px;
    font-size: 0.6em;
    color: #a3a3a3;
    text-decoration: line-through;
}

.earnRed {
    color: #ff0000;
}

.earn {
    font-size: 0.9em;
}
/* 商品金额 */
.price{
    box-sizing: border-box;
    width: 94%;
    background: #fff;
    margin: 0 auto;
    padding: 10px;
    border-radius: 5px;
    font-size: 0.8em;
    line-height: 20px;
}
.priceItem{
    display: flex;
    justify-content: space-between;
}
.priceTotal{
    display: flex;
    justify-content: flex-end;
    color: #ff0000;
    margin-top: 8px;
    align-items: baseline;
}
.priceNum{
    font-size: 1.2em;
}
/* 联系客服 */
.service{
    box-sizing: border-box;
    width: 94%;
    background: #ffffff;
    margin: 1px auto;
    padding: 10px;
    border-radius: 5px;
    font-size: 0.9em;
    line-height: 20px;
    display: flex;
    justify-content: space-between;

}
/* 订单编号等 */
.nub{
    box-sizing: border-box;
    width: 94%;
    background: #ffffff;
    margin: 7px auto;
    padding: 10px;
    border-radius: 5px;
    font-size: 0.8em;
    line-height: 20px;
}
.nubFlex{
    display: flex;
    justify-content: space-between;
}
.copy{
    border: 1px solid #000;
    border-radius: 6px;
    padding: 0 6px;
}