html,body{
    background: #f2f2f2;
}
.page-head{
    display: flex;
    height: 44px;
    line-height: 44px;
}
.head-center{
    margin: 0 auto;
}
.back{
    position: absolute;
    left: 10px;
    top:50%;
    transform: translateY(-50%)
}
/* 用户地址 */
.userAdd{
    width: 96%;
    margin:8px 2%;
    background: #fff;
    border-radius: 8px;
    display: flex;
    justify-content: center;
    padding: 30px 0;
}
.addBtn{
    font-size: 0.6em;
    border: 1px solid #000;
    border-radius: 20px;
    padding: 2px 10px;
}
.userAdd1{
    width: 96%;
    margin:8px 2%;
    background: #fff;
    border-radius: 8px;
    padding: 10px;
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
}

.addIcn{
    width: 12%;
}
.orderText{
    font-size: 0.6em;
}
.orderName{
    font-size: 0.9em;
    margin-right: 10px;
    line-height: 25px;
}
.orderCall{
    color: #a3a3a3;
}
.iconRt{
    margin-top: 8px;
}
/* 购买的产品 */
.product{
    width: 96%;
    background: #fff;
    border-radius: 8px;
    margin: 0 2%;
    font-size: 0.9em;
    padding-bottom: 15px;
}
.chef-form-card{
    display: flex;
    padding: 10px;
    border-bottom: 1px solid #f2f2f2;
}
.chef-form-leftimg{
    width: 120px;
    height: 110px;
    margin-right: 10px;
    border-radius: 4px;
    background: skyblue;
}
.chef-form-leftimg img{
    width: 100%;
    height: 100%;
    border-radius: 4px;
}
.chef-right-content{
    line-height: 20px;
}
.chef-head1{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.chef-head2{
    color:#a3a3a3 ;
    font-size: 0.6em;
}
 .chef-head3 {
    font-size: 0.6em;
    color: #a3a3a3;
}
.forum-foot-bg{
    background: #d96b6c;
    padding: 0 5px;
    margin-right: 2px;
    color: #fff;
    border-radius: 4px;
}
.chef-head4{
    margin-top: 30px;
}
.swiper-red{
    color: #ff0000;
    position: relative;
    width: 100%;
}
.swiper-del{
    margin-left: 3px;
    font-size: 0.6em;
    color: #a3a3a3;
    text-decoration:line-through;
}
.swiper-nub{
    position: absolute;
    right: 0;
    color: #000;
}
/* 购买资料 */
.productInp-item{
    width: 100%;
    display: flex;
    position: relative;
    line-height: 40px;
}
.tabName{
    width: 30%;
    text-align: right;
    margin-right: 15px;
}
.numberBox{
    position: absolute;
    right: 10px;
    bottom: 12px;
    border: 1px solid #999;
    border-radius: 3px;
    line-height: 17px;
}
.nubBox{
    font-size: 0.6em;
    padding: 0 10px;

}
.count{
    padding: 0 7px;
    border-right: 1px solid #a3a3a3;
    color: #a3a3a3;
}
.count1{
    padding: 0 7px;
    border-left: 1px solid #a3a3a3;
    color: #a3a3a3;
}
.option{
    width: 55%;
    margin-right: 10px;
    text-align: right;
}
/* 选择付款方式 */
.payment{
    width: 91%;
    margin:8px 2%;
    background: #fff;
    border-radius: 8px;
    padding: 10px;
}
.paymentText{
    font-size: 0.8em;
    line-height: 16px;
}
.describe{
    font-size: 0.8em;
    color: #a3a3a3;
}
.ui-checkbox input:checked:before, 
.ui-checkbox-s input:checked:before{
    color: #6eb92c;
}
/* 底部浮动按钮 */
.submission{
    width: 100%;
    position: fixed;
    bottom: 0;
    display: flex;
    justify-content: flex-end;
    background: #ffffff;
    border-top: 1px solid #f2f2f2;
}
.submissionPrice{
    font-size: 0.8em;
    color: #a3a3a3;
    padding-right: 20px;
}
.submissionB{
    color: #000;
    font-size: 14px;
}
.submissionR{
    color: #ff0000;
    font-size: 14px;
}
.discount{
    text-align: right;
}
.submissionBtn{
    width: 33%;
    line-height: 40px;
    background:#6eb92c;   
}
/* 优惠券弹窗 */
.popup{
    font-size: 0.9em;
    border-top-right-radius:10px;
    border-top-left-radius: 10px;   
    background: #ffffff;  
}
.popupDiscount{
    display: flex;
    justify-content: space-between;
    line-height: 50px;
    padding: 0 15px;
    border-bottom: 1px solid #f2f2f2;
    margin-bottom: 15px;
}
.popupItem{
    width: 94%;
    display: flex;
    justify-content: center;
    margin: 10px auto;
    line-height: 18px;
}
.popupCoupon{
    background: #fbeded;
    width: 70%;
    padding: 5px 15px;
    border-radius: 8px;
}
.popupDate{
    font-size: 0.8em;
    color: #a3a3a3;
}
.receive{
background: #b96d6c;
color: #ffffff;
width: 30%;
border-radius: 8px;
}
.popupBtn{
    width: 100%;
    background: #6eb92c;
    color: #ffffff;
    line-height: 40px;
    text-align: center;
    margin-top: 30px;
}